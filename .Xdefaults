URxvt.font: xft:PragmataPro for Powerline:pixelsize=12;xft:Sazanami Gothic,さざなみゴシック:pixelsize=10:style=Bold

URxvt.depth: 24
*scrollstyle: plain
*scrollBar: false
*scrollBar_floating: true
*scrollBar_right: true

*internalBorder: 4

Xft*dpi: 96
Xft*antialias: true
Xtf*hinting: full

! special
*.foreground:   #ffffff
*.background:   #454545
*.cursorColor:  #ffffff

! black
*.color0:       #5a5a5a
*.color8:       #8c8c8c

! red
*.color1:       #fcd6d5
*.color9:       #fecbc9

! green
*.color2:       #fbdfe5
*.color10:      #fbced9

! yellow
*.color3:       #f3e1cd
*.color11:      #fdf0c8

! blue
*.color4:       #d7f3f8
*.color12:      #a2d4de

! magenta
*.color5:       #9ec6f6
*.color13:      #7fc8e6

! cyan
*.color6:       #91cee6
*.color14:      #4bc0c6

! white
*.color7:       #d4d4d4
*.color15:      #f8f8f8

